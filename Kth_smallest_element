package practise;
import java.util.Arrays;
import java.util.Scanner;
public class main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		int n = sc.nextInt();
		int a[] = new int[n];
		
		for(int i=0; i<n; i++) {
			a[i]=sc.nextInt();
		}
		
		int k = sc.nextInt();
		Arrays.sort(a);
		
		for(int i=0; i<=n; i++) {
			System.out.println(a[k-1]);
			break;
		}
		
		sc.close();
		
	}
}
